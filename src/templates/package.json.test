{
  "scripts": {
    "test": "mocha -r ts-node/register --config .mocharc.js",
    "test:ui": "mocha -r ts-node/register --config .mocharc.js 'test/ui/**/*.test.ts'",
    "test:unit": "mocha -r ts-node/register --config .mocharc.js 'test/unit/**/*.test.ts'",
    "compile:test": "tsc -p ./tsconfig.test.json",
    "pretest": "npm run compile && npm run compile:test",
    "package": "vsce package -o ./dist/extension.vsix",
    "pretest:ui": "npm run package"
  },
  "devDependencies": {
    "@salesforce/salesforcedx-vscode-test-tools": "^1.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.7",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vsce": "^2.7.0",
    "vscode-extension-tester": "^8.0.0"
  }
}
